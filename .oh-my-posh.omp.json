{
	"$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
	"final_space": true,
  "blocks": [
    {
      "type": "prompt",
			"alignment": "right",
      "segments": [
        {
					"type": "path",
					"style": "diamond",
					"trailing_diamond": "\uE0B4",
					"leading_diamond": "\uE0B6",
					"foreground": "#3EC669",
					"background": "#29315A",
					"properties": {
						"home_icon": "$HOME",
						"prefix": "",
						"postfix": "",
						"style": "agnoster",
						"mapped_locations": {}
					}
        },
				{
          "type": "git",
					"style": "diamond",
					"trailing_diamond": "\uE0B4",
					"leading_diamond": " \uE0B6",
					"foreground": "#43CCEA",
					"background": "#29315A",
					"properties": {
						"prefix": "",
						"postfix": "",
						"branch_icon": "",
						"template": "{{ .HEAD }}"
					}
				},
				{
					"type": "time",
					"style": "diamond",
					"trailing_diamond": "\uE0B4",
					"leading_diamond": " \uE0B6",
					"foreground": "#007ACC",
					"background": "#29315A",
					"properties": {
						"time_format": "15:04:05"
					}
				}
			]
    },
		{
      "type": "prompt",
			"alignment": "left",
			"newline": true,
      "segments": [
        {
					"type": "executiontime",
					"style": "diamond",
					"trailing_diamond": "\uE0B4 ",
					"leading_diamond": "\uE0B6",
					"foreground": "#AEA4BF",
					"background": "#29315A",
					"properties": {
						"threshold": 150,
						"style": "austin",
						"prefix": "",
						"postfix": ""
					}
				},
				{
					"type": "session",
					"style": "diamond",
					"trailing_diamond": "\uE0B4 ",
					"leading_diamond": "\uE0B6",
					"foreground": "#E64747",
					"background": "#29315A",
					"properties": {
						"display_host": false,
						"postfix": " \u276F",
						"prefix": ""
					}
				}
      ]
    }
  ]
}